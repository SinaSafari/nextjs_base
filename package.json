{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "sinasafari",
    "email": "m.sinasafari@gmail.com"
  },
  "license": "GPLv3",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/server.js",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --coverage --silent --ci",
    "cypress:open": "cypress open",
    "cypress:open:server": "start-server-and-test dev 3000 cypress:open",
    "cypress:run": "cypress run",
    "m:make": "npx knex migrate:make --stub ../lib/stubs/migration.js --knexfile ./src/database/knexfile.js",
    "m:run": "npx knex migrate:latest --knexfile ./src/database/knexfile.js",
    "s:make": "npx knex seed:make --stub ../lib/stubs/seeder.js --knexfile ./src/database/knexfile.js",
    "s:run": "npx knex seed:run --knexfile ./src/database/knexfile.js"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.16",
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@reduxjs/toolkit": "^1.6.2",
    "express": "^4.17.1",
    "framer-motion": "^4.1.17",
    "knex": "^0.21.21",
    "next": "12.0.1",
    "next-auth": "^4.0.0-beta.5",
    "objection": "^2.2.17",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "cypress": "^8.7.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.1",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-testing-library": "^5.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.43.4",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "4.4.4"
  }
}
